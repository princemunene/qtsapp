<?xml version='1.0' encoding='utf-8'?>
<widget id="com.qet.eazzypos" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <name>EazzyPos</name>
    <description>
        A Retail Point of Sale Application.
    </description>
    <author email="info@qet.co.ke" href="http://cordova.io">
        QET SYSTEMS LTD
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon src="www/images/icon.png" />
</widget>
